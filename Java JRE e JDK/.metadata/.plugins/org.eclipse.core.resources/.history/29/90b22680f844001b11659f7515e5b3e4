
public class TesteSaca {

	public static void main(String[] args) throws SaldoInsuficienteException{
	
		Conta c = new ContaCorrente(12, 12);
		
		c.deposita(250);
		//c.saca(270);
		
		try {
			c.saca(270);
		}catch(Exception ex) {
			System.out.println("Erro");
			 ex.printStackTrace();
		}
		System.out.println(c.getSaldo());
	}
}
